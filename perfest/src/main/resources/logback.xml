<!--
Smile CDR uses the Logback framework for logging. For details on configuring this
file, see:
https://smilecdr.com/docs/logging/troubleshooting_logs.html
-->
<configuration>

	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %logger{36} - %msg%n</pattern>
		</encoder>
	</appender>

	<root level="INFO">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>
